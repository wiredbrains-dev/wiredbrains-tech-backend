        "shield",
        "server",
        "cloud",
        "monitor",
        "lock",
        "settings",
        "phone",
        "database",
        "refresh-cw",
        "mail",
        "headphones",
        "network",
        "hard-drive",
        "cpu",
        "key"
      ],
      "default": "settings"
    },
    "details": {
      "type": "json"
    },
    "display_order": {
      "type": "integer",
      "default": 0
